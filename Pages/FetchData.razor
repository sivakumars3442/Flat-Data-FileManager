@page "/fetchdata"

@*Initializing File Manager with Firebase Realtime Database service*@

@* Replace the hosted port number in the place of "{port}" *@

<SfFileManager TValue="FileManagerDirectoryContent">
    <FileManagerAjaxSettings Url="https://localhost:62870/api/FirebaseProvider/FirebaseRealtimeFileOperations"
                             UploadUrl="https://localhost:62870/api/FirebaseProvider/FirebaseRealtimeUpload"
                             DownloadUrl="https://localhost:62870/api/FirebaseProvider/FirebaseRealtimeDownload"
                             GetImageUrl="https://localhost:62870/api/FirebaseProvider/FirebaseRealtimeGetImage">
    </FileManagerAjaxSettings>
	<FileManagerEvents TValue="FileManagerDirectoryContent" BeforeImageLoad="@BeforeImageLoad" BeforeDownload="BeforeDownload"></FileManagerEvents>
</SfFileManager>
