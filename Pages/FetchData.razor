@page "/fetchdata"

@*Initializing File Manager with Firebase Realtime Database service*@

@* Replace the hosted port number in the place of "{port}" *@

<SfFileManager TValue="FileManagerDirectoryContent">
		<FileManagerAjaxSettings Url="https://localhost:62871/api/AmazonS3Provider/AmazonS3Fileoperations"
								 UploadUrl="https://localhost:62871/api/AmazonS3Provider/AmazonS3Upload"
								 DownloadUrl="https://localhost:62871/api/AmazonS3Provider/AmazonS3Download"
								 GetImageUrl="https://localhost:62871/api/AmazonS3Provider/AmazonS3GetImage">
		</FileManagerAjaxSettings>
	<FileManagerEvents TValue="FileManagerDirectoryContent" BeforeImageLoad="@BeforeImageLoad" BeforeDownload="BeforeDownload"></FileManagerEvents>
</SfFileManager>
